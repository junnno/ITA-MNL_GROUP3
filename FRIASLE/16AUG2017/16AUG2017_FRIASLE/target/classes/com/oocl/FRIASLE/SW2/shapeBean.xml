<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="Square" class="com.oocl.FRIASLE.SW2.Square">
		<property name="name" value="Square"></property>
	</bean>
	<bean id="Triangle" class="com.oocl.FRIASLE.SW2.Triangle">
		<property name="name" value="Triangle"></property>
	</bean>
	<bean id="Circle" class="com.oocl.FRIASLE.SW2.Drawing">
		<property name="shape">
			<bean class="com.oocl.FRIASLE.SW2.Circle">
			</bean>
		</property>
	</bean>

	<bean id="Oct" class="com.oocl.FRIASLE.SW2.Drawing">
		<property name="shape">
			<bean class="com.oocl.FRIASLE.SW2.Oct">
			</bean>
		</property>
	</bean>
</beans>